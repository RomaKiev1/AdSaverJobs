  <schedule>

    <job>
      <name>SetKeywordsCostJob</name>
      <group>SetKeywordsCostJobGroup</group>
      <description>SetKeywordsCostJob</description>
      <job-type>Quartz.Server.Jobs.SetKeywordsCostJob, CallTrackingJobs</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>SetKeywordsCostJobTriggerr</name>
        <group>SetKeywordsCostJobTriggerrGroup</group>
        <description>SetKeywordsCostJobTriggerr</description>
        <job-name>SetKeywordsCostJob</job-name>
        <job-group>SetKeywordsCostJobGroup</job-group>
        <cron-expression>0 0 16 * * ?</cron-expression>
      </cron>
    </trigger>
    
